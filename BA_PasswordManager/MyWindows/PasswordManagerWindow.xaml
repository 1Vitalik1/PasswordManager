<Window x:Class="BA_PasswordManager.MyWindows.PasswordManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BA_PasswordManager.MyWindows"
        mc:Ignorable="d"
        Title="PasswordManagerWindow" Height="720" Width="1280"
        WindowStartupLocation="CenterScreen" WindowStyle="None" Background="Transparent" AllowsTransparency="True" MouseDown="Window_MouseDown">
    <Border Background="#1D2028" CornerRadius="5" Margin="5">
        <Border.Effect>
            <DropShadowEffect Color="#366EC0" ShadowDepth="0" BlurRadius="15">
            </DropShadowEffect>
        </Border.Effect>
        <Frame x:Name="frame"  NavigationUIVisibility="Hidden"></Frame>
    </Border>
</Window>
